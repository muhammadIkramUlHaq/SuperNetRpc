#! /bin/bash

echo "RPC:decoderawtransaction"
echo "The decoderawtransaction RPC decodes a serialized transaction hex string into a JSON object describing the transaction"
echo "Parameter #1—serialized transaction in hex"
echo ""
echo "Result—the decoded transaction"

curl --url "http://127.0.0.1:7778" --data "{\"method\":\"decoderawtransaction\",\"params\":[\"\"]}"
